{
  "name": "dibs-vg",
  "version": "1.9.0",
  "description": "1stdibs's in-house vector graphics.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --port 9090 --content-base=./ --inline --watch --hot",
    "compile:webpack": "webpack -d --progress",
    "compile": "node ./generateList && node ./generateDist",
    "build": "npm run compile && npm run compile:webpack",
    "gitlint": "node_modules/.bin/dibslint --git --warnings --es6",
    "lint": "dibslint --root=. --es6",
    "prepublish": "npm run compile"
  },
  "pre-commit": {
    "run": [
      "gitlint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1stdibs/dibs-vg.git"
  },
  "keywords": [
    "SVG",
    "vector",
    "graphics",
    "1stdibs",
    "1stdibs.com"
  ],
  "author": "1stdibs FE eng",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/1stdibs/dibs-vg/issues"
  },
  "homepage": "https://github.com/1stdibs/dibs-vg#readme",
  "dependencies": {
    "backbone": "^1.3.3",
    "jquery": "^2.2.3",
    "react": "^15.0.2",
    "react-dom": "^15.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.8.0",
    "css-loader": "^0.23.1",
    "dibslint": "^2.0.3",
    "html-loader": "^0.4.3",
    "json-loader": "^0.5.4",
    "jsx-loader": "^0.13.2",
    "node-sass": "^3.7.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "pre-commit": "^1.1.3",
    "precss": "^1.4.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "svg-react-loader": "^0.3.3",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "xml2js": "^0.4.16"
  }
}
